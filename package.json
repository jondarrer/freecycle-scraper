{
  "name": "freecycle-scraper",
  "version": "1.0.0",
  "description": "Crawl and scrape the Freecycle website",
  "main": "src/index.js",
  "scripts": {
    "lint:js": "eslint app/ webpack.*.js --cache",
    "test": "jest",
    "test-jenkins": "jest --colors --config ./jest.jenkins.config.js",
    "build": "webpack",
    "precommit": "npm run lint:js",
    "prepush": "npm run lint:js && npm test"
  },
  "repository": "jondarrer/freecycle-scraper",
  "author": "Jonathan Darrer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jondarrer/freecycle-scraper/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/jondarrer/freecycle-scraper#readme",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "cheerio": "^1.0.0-rc.2",
    "pg": "6.1.5",
    "source-map-support": "^0.5.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "jest": "^23.6.0",
    "jest-jenkins-reporter": "^1.0.2",
    "mockery": "^2.1.0",
    "nock": "^11.7.0",
    "sinon": "^5.0.7",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "report.xml",
    "indent": 4
  }
}
